    "def exp(x):\n",
    "    genkou = 1\n",
    "    exp = 1\n",
    "    for n in range(10):\n",
    "        genkou *= x/n\n",
    "        exp += genkou\n",
    "    return round(exp, 8)\n",
    "\n",
    "\n",
    "if x < 0:\n",
    "    x = -x\n",
    "    print('1/exp(|x|)=', exp(x))\n",
    "else:\n",
    "    print('exp(x)=', exp(x))"
   ]
